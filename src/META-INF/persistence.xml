<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="AE_TrackerPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>model.Log</class>
    <class>model.Subjects</class>
    <class>model.Directorate</class>
    <class>model.User</class>
    <class>model.Notification</class>
    <class>model.Patient</class>
    <class>model.Team</class>
    <class>model.Superadmin</class>
    <class>model.DutyRoster</class>
    <class>model.Department</class>
    <class>model.Staff</class>
    <class>model.PublicInfo</class>
    <class>model.Diagnosis</class>
    <class>model.Admin</class>
    <class>model.Appointment</class>
    <class>model.StaffHasTeam</class>
    <class>model.Duty</class>
    <class>model.Position</class>
    <class>model.StaffHasDuty</class>
    <class>model.Guest</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/lab_pro?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
    </properties>
  </persistence-unit>
</persistence>
